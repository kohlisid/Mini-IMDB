{
    "manifest_version": 2,
    "name": "Mini-IMDB",
    "description": "A extention to search IMDB data",
    "version": "1.0",
    "icons": { 
        "16": "imdb-icon.png",
        "48": "imdb-icon.png",
        "128": "imdb-icon.png" 
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}